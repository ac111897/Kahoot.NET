<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>10</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <SignAssembly>False</SignAssembly>
    <PackageProjectUrl>https://github.com/ac111897/Kahoot.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ac111897/Kahoot.NET</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Kahoot;.NET;WebSockets</PackageTags>
    <NeutralLanguage>en-NZ</NeutralLanguage>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>Bradley Grover</Authors>
    <Version>1.0.1</Version>
    <Description>Library to connect and interact with Kahoot's using .NET</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReleaseNotes>First publish</PackageReleaseNotes>
    <PackageId>Paradox.Kahoot.NET</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <Content Remove="C:\Users\bradl\.nuget\packages\flee\2.0.0\contentFiles\any\net6.0\Resources\DocComments.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Flee" Version="2.0.0" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="6.0.0" />
    <PackageReference Include="NuGet.Build" Version="2.12.1" />
    <PackageReference Include="OneOf" Version="3.0.216" />
    <PackageReference Include="RandomUserAgent" Version="1.1.2" />
    <PackageReference Include="SpanExtensions" Version="1.0.0" />
    <PackageReference Include="System.Text.Json" Version="6.0.5" />
  </ItemGroup>

  <ItemGroup>
    <EditorConfigFiles Remove="F:\repos\Kahoot.NET\Kahoot.NET\.editorconfig" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="API\Responses\Json\" />
  </ItemGroup>

</Project>
